status: hidden
template: self

<%
import string

try:
  import ujson as json
except ImportError:
  import json

reduced_list = []
for item in pages:
  if item['status'] in ['hidden', 'draft'] \
    or item['url'].endswith('.json'):
    continue
  if 'tags' in item:
    reduced_list += item['tags']

reduced_list = set([string.capwords(str(k)) for k in reduced_list if len(k) >= 3])

%>${ json.dumps(reduced_list) }
